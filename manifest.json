{
  "manifest_version": 3,
  "name": "Search Master",
  "version": "1.3.0",
  "key": "",
  "description": "Intelligently routes searches between multiple search engines (Google, Bing, ChatGPT, Perplexity) based on your preferences",
  "permissions": [
    "storage",
    "webNavigation"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "*://www.google.com/*",
    "*://chatgpt.com/*",
    "*://www.perplexity.ai/*",
    "*://www.bing.com/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://*.chatgpt.com/*"],
      "js": ["content_script.js"],
      "run_at": "document_start"
    }
  ]
}
